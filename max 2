#include <stdio.h>

int main()
{
    int a, b;
    printf("Enter the first number:");
    scanf("%d", &a);
    printf("Enter the second number:");
    scanf("%d", &b);
    
    if (a>b) {
        printf("A is greater");
        } else if (b>a);
        {
        printf("B is greater");
        }
        
        
     return 0;
} 
