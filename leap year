#include <stdio.h>

int main()
{
    int a;
    scanf("%d", &a);
    if (a%400==0); {
      printf("given no is leap year");
    }  else if  (a%100!= 0 && a%4 == 0) {
      printf("given no is leap year");
    } else { 
      printf("given number is not a leap year");
      
    }
      
    return 0;
}
